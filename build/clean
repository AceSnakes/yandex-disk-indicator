#!/bin/bash
cd yd-tools/
rm opt/yd-tools/yandexDiskIndicator.py
rm opt/yd-tools/ya-setup
rm opt/yd-tools/icons/yd-128.png
rm opt/yd-tools/icons/dark/yd-busy1.png
rm opt/yd-tools/icons/dark/yd-busy2.png
rm opt/yd-tools/icons/dark/yd-busy3.png
rm opt/yd-tools/icons/dark/yd-busy4.png
rm opt/yd-tools/icons/dark/yd-busy5.png
rm opt/yd-tools/icons/dark/yd-ind-error.png
rm opt/yd-tools/icons/dark/yd-ind-idle.png
rm opt/yd-tools/icons/dark/yd-ind-pause.png
rm opt/yd-tools/icons/light/yd-busy1.png
rm opt/yd-tools/icons/light/yd-busy2.png
rm opt/yd-tools/icons/light/yd-busy3.png
rm opt/yd-tools/icons/light/yd-busy4.png
rm opt/yd-tools/icons/light/yd-busy5.png
rm opt/yd-tools/icons/light/yd-ind-error.png
rm opt/yd-tools/icons/light/yd-ind-idle.png
rm opt/yd-tools/icons/light/yd-ind-pause.png
rm opt/yd-tools/fm-actions/Nautilus_Nemo/publish
rm opt/yd-tools/fm-actions/Nautilus_Nemo/unpublish

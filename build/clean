rm usr/share/locale/el/LC_MESSAGES/yandex-disk-indicator.mo
rm usr/share/applications/Yandex.Disk.desktop
rm usr/share/applications/Yandex.Disk-indicator.desktop

rm debian/yd-tools
cd ..